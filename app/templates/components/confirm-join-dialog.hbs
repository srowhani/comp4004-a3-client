{{#paper-dialog fullscreen=fullscreen onClose=onClose origin=dialogOrigin}}

  {{#paper-dialog-content}}
    <h2 class="md-title">Join room: {{room.room_id}}?</h2>
  {{/paper-dialog-content}}

  {{#paper-dialog-actions class="layout-row"}}
    <span class="flex"></span>
    {{#paper-button primary=true onClick=onClose}}Cancel{{/paper-button}}
    {{#paper-button primary=true onClick=onSubmit}}Join!{{/paper-button}}
  {{/paper-dialog-actions}}

{{/paper-dialog}}
